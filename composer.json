{
    "name": "dynamic/silverstripe-elemental-markdown",
    "description": "Markdown Slideshow content block for SilverStripe Elemental.",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "markdown",
        "elemental"
    ],
    "license": "BSD-3-Clause",
    "require": {
        "dnadesign/silverstripe-elemental": "^4.0",
        "undefinedoffset/silverstripe-markdown": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Dynamic\\Elements\\Markdown\\": "src/",
            "Dynamic\\Elements\\Markdown\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
